{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ],
        [
            "@babel/preset-react",
            {
                "runtime": "automatic"
            }
        ],
        "@babel/typescript"
    ],
    "env": {
        "production": {
            "plugins": [
                "@emotion"
            ]
        },
        "test": {
            "presets": [
                [
                    "@babel/preset-env"
                ],
                "@babel/preset-typescript"
            ]
        }
    },
    "plugins": [
        "@emotion"
    ]
}